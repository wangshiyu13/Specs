{
  "name": "CygnusReach",
  "version": "1.5.0-beta6",
  "source": {
    "http": "https://github.com/cygnus-technology/reach-ios-framework/releases/download/v1.5.0-beta6/CygnusReach.zip"
  },
  "license": {
    "type": "PRIVATE",
    "file": "CygnusReach/LICENSE"
  },
  "summary": "SDK to enable easy remote support sessions",
  "description": "Reduce support call time and increase customer satisfaction for your internet of things solutions with Reachâ„  remote support.",
  "platforms": {
    "ios": "13"
  },
  "authors": "Cygnus",
  "homepage": "https://cygnustechnology.com",
  "social_media_url": "https://www.linkedin.com/company/cygnus-iot",
  "default_subspecs": "Core",
  "dependencies": {
    "PromiseKit": [
      "~> 8"
    ]
  },
  "swift_versions": "5.10",
  "static_framework": false,
  "user_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "OTHER_SWIFT_FLAGS": "-no-verify-emitted-module-interface"
  },
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "CygnusReach/Frameworks/RemoteSupport.xcframework",
      "dependencies": {
        "CocoaMQTT": [
          "~> 2.1.3"
        ],
        "WebRTC-lib": [
          "~> 120.0.0"
        ]
      }
    },
    {
      "name": "ReachProtocol",
      "vendored_frameworks": "CygnusReach/Frameworks/ReachProtocol.xcframework",
      "dependencies": {
        "SwiftProtobuf": [
          "~> 1.27.0"
        ]
      }
    },
    {
      "name": "UI",
      "vendored_frameworks": "CygnusReach/Frameworks/CygnusUI.xcframework",
      "dependencies": {
        "CygnusReach/Core": [

        ],
        "CygnusReach/ReachProtocol": [

        ],
        "Toast-Swift": [
          "~> 5.1.1"
        ]
      }
    }
  ],
  "swift_version": "5.10"
}
