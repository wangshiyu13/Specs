{
  "name": "epotheke-sdk",
  "version": "1.1.3-rc.2",
  "summary": "epotheke sdk",
  "description": "iOS framework for integration of Epotheke services",
  "homepage": "https://www.epotheke.com",
  "authors": {
    "Name": "florian.otto@ecsec.de"
  },
  "license": {
    "type": "GPLv3",
    "file": "LICENSE"
  },
  "source": {
    "http": "https://github.com/epotheke/epotheke-sdk/releases/download/v1.1.3-rc.2/Epotheke-CocoaPod.zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "preserve_paths": "Epotheke.xcframework/*",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATH[sdk=iphoneos*]": "$(inherited) \"$(PODS_ROOT)/Epotheke\"",
    "OTHERCFLAGS[sdk=iphoneos*]": "$(inherited) -iframework \"$(PODS_ROOT)/Epotheke\"",
    "OTHER_LDFLAGS[sdk=iphoneos*]": "$(inherited) -framework Epotheke"
  }
}
